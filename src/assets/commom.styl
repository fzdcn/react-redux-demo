
.clearfix:after
    content '.'
    display block
    height 0
    visibility hidden
    clear both
.fr
    float right
.fl
    float left
.clear
    clear both
.text-center
    text-align center
.text-left
    text-align left
.text-right
    text-align right
.text-bold
    font-weight bold
a
    text-decoration none
a:focus
    outline 0
ul, ol, li
    list-style none
    padding 0
    margin 0
h1, h2, h3, h4, h5, h6, p
    margin 0
    padding 0
    font-weight normal
img
    max-width 100%
    border 0
    vertical-align middle
dl
    margin 0
dd
    margin 0
input, select, textarea, button
    outline none
    border none
input, textarea, select
    &:-webkit-autofill
        -webkit-text-fill-color #606266 !important
        -webkit-box-shadow 0 0 0px 1000px transparent inset !important
        background-color transparent
        background-image none
        transition background-color 50000s ease-in-out 0s // 背景色透明  生效时长  过渡效果  启用时延迟的时间
textarea
    resize none
.cursor
    cursor pointer
